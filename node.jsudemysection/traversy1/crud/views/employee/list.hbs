<h3><a class="btn btn-secondary" href="/employee"><i class="fa fa-plus"></i> Create New</a> Employee List</h3>
<table class="table table-striped">
    <thead>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>#</th>
    </tr>
</thead>
<tbody>
    {{#each list}}
    <tr>
        <td>{{first_Name}}</td>
        <td>{{last_Name}}</td>
        <td>
           <a href="/employee/{{_id}}"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i>Edit</a>
           <a href="/employee/delete/{{_id}}" onclick="return confirm('Are you sure to delete this record ?');"><i class="fa fa-trash fa-lg" aria-hidden="true">Delete</i></a>
            </td>
    </tr>
    {{/each}}
</tbody>
</table>